{
    "name": "speed/speedadmmin",
    "description": "SpeedAmin是一个基于thinkphp6和layui的快速开发应用框架",
    "type": "project",
    "license": "Apache-2.0",
    "keywords": [
        "speed-admmin",
        "FunAdmin",
        "easywechat",
        "framework",
        "thinkphp",
        "thinkphp6",
        "ORM"
    ],
    "homepage": "https://www.speedadmmin.com",
    "authors": [
        {
            "name": "yue909",
            "email": "speed@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "topthink/framework": "6.0.*-dev",
        "topthink/think-orm": "2.0.*-dev",
        "topthink/think-view": "^1.0",
        "topthink/think-image": "^1.0",
        "topthink/think-queue": "^3.0",
        "topthink/think-captcha": "^3.0",
        "topthink/think-helper": "^3.1",
        "topthink/think-multi-app": "^1.0",
        "overtrue/wechat": "^4.2",
        "ramsey/uuid": "^3.8",
        "phpmailer/phpmailer": "^6.1",
        "aliyuncs/oss-sdk-php": "^2.3",
        "qcloud/cos-sdk-v5": "^2.0",
        "qiniu/php-sdk": "^7.2",
        "ext-json": "*",
        "funadmin/fun-addons": "v0.31",
        "topthink/think-ide-helper": "^1.0",
        "topthink/think-annotation": "^1.1",
        "topthink/think-throttle": "^1.1",
        "james-heinrich/getid3": "^1.9"
    },
    "require-dev": {
        "symfony/var-dumper": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "app\\": "app"
            },

        "psr-0": {
            "": "extend/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "scripts": {
        "post-autoload-dump": [
            "@php think service:discover",
            "@php think vendor:publish"
        ]
    }

}
